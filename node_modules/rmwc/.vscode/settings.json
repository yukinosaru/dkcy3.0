{
  "git.ignoreLimitWarning": true,
  "flow.useNPMPackagedFlow": true,
  "javascript.validate.enable": false,
  "flow.runOnEdit": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "cSpell.words": [
    "alertdialog",
    "checkboxes",
    "deregister",
    "eqeqeq",
    "fabs",
    "lastrow",
    "listbox",
    "mdc's",
    "refname",
    "reinit",
    "rmwc",
    "scroll",
    "scroller",
    "snackbars",
    "submenu",
    "truthy",
    "unelevated",
    "unminified",
    "unmounting"
  ],
  "cSpell.allowCompoundWords": true
}
